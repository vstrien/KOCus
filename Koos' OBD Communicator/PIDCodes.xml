      <PIDSensor PID="0A" bytes="01" Description="Fuel pressure (kPa - gauge)" Formula="A*3" />
      <PIDSensor PID="0B" bytes="01" Description="Intake manifold absolute pressure (kPa - absolute)" Formula="A" />
      <PIDSensor PID="0C" bytes="02" Description="Engine RPM" Formula="((A*256)+B)/4" />
      <PIDSensor PID="0D" bytes="01" Description="Vehicle speed (km/h)" Formula="A" />
      <PIDSensor PID="0E" bytes="01" Description="Timing advance (degrees relative to #1 cylinder)" Formula="A/2 - 64" />
      <PIDSensor PID="0F" bytes="01" Description="Intake air temperature" Formula="A-40" />
      <PIDSensor PID="10" bytes="02" Description="MAF air flow rate (grams / sec)" Formula="((A*256)+B) / 100" />
      <PIDSensor PID="11" bytes="01" Description="Throttle position (%)" Formula="A*100/255" />
      <PIDSensor PID="12" bytes="01" Description="Commanded secondary air status" />
      <PIDSensor PID="13" bytes="01" Description="Oxygen sensors present" />

